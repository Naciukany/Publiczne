command /helpop <text>:
    description: BY MAXHARD
    usage: /helpop <message>
    aliases: /helpadmin
    permission: gracz.helpop
    trigger:
        loop all players:
            loop-player has permission "admin.helpop":
                message "&8[&9Pomocy&8] &a%player%&f: %arg%" to loop-player
                set {helpop.%player%} to false
                        wait 1 minute
                        set {helpop.%player%} to true
on command "/helpop":
        if {helpop.%player%} is false:
                cancel event
                send "{@tag} &cNa helpop mozna pisac co 1 minute."
